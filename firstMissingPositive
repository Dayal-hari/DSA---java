class Solution {
    public int firstMissingPositive(int[] nums) {
        int n=nums.length;
        for(int i = 1; i <= nums.length + 1; i++) {
            boolean found = false;

            for (int j = 0; j < nums.length; j++) {
                if (nums[j] == i) {
                    found = true;
                    break;
                }
            }

            if (!found) {
                return i;
            }
        }
        return 1;
    

        
    }
}
