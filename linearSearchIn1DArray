public class linearSearchIn1DArray {
    public static void main(String[]args){
        int[]arr={18,12,9,14,77,50};
        int target=14;
        boolean found=false;
        for(int i=0;i<arr.length;i++){
            if(arr[i]==target){
                found=true;
            }
        }
        System.out.println("found:"+found);
    }
}
